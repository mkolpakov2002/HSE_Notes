<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.HSENotes" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Customize your theme here. -->
        <item name="colorControlActivated">?attr/colorAccent</item>
        <item name="android:colorControlActivated">?attr/colorAccent</item>
        <item name="colorPrimaryContainer">?attr/colorSurface</item>
        <item name="colorOnPrimaryContainer">?attr/colorPrimary</item>
        <item name="android:statusBarColor">?android:attr/windowBackground</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowActionBar">false</item>

    </style>
</resources>